/**
 * For emitting .d.ts files for typescript libraries intended for the browser
 *
 * @example ui component libraries, utilities for applications, etc.
 */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "declaration": true,
    "emitDeclarationOnly": true,
    "lib": ["dom", "ESNext"]
  }
}
