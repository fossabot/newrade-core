<% const package = require('../../../../package.json')%>

<!-- THEME COLOR OVERRIDES MANIFEST THEME_COLOR -->
<meta name="theme-color" content="#7C997D">
<meta name="application-name" content="<%=package.name%>">

<!-- INSTALLABLE WEB APP ON ANDROID (IGNORED IF MANIFEST.JSON IS FOUND) -->
<!-- https://developer.chrome.com/multidevice/android/installtohomescreen -->
<meta name="mobile-web-app-capable" content="yes">

<!-- INSTALLABLE WEB APP ON iOS -->
<!-- https://developer.apple.com/library/content/documentation/AppleApplications/Reference/SafariHTMLRef/Articles/MetaTags.html -->
<meta name="apple-mobile-web-app-capable" content="yes">
<meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
<meta name="apple-mobile-web-app-title" content="VSB Admin">

<!-- MICROSOFT TILES -->

<meta name="msapplication-config" content="browserconfig.xml" />
<meta name="msapplication-navbutton-color" content="#fff">
<meta name="msapplication-starturl" content="/">
