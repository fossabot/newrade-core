{
  "name": "@newrade/core-react-ui",
  "version": "0.0.0",
  "description": "React UI library implementing the core-design-system",
  "license": "MIT",
  "scripts": {
    "build": "yarn lerna:build:lib --scope @newrade/core-react-ui-*",
    "clean": "yarn lerna:clean:lib --scope @newrade/core-react-ui-*",
    "lerna:build:lib": "lerna run --stream build:lib ",
    "lerna:clean:lib": "lerna run --stream clean:lib ",
    "lerna:start:lib": "lerna run --parallel start:lib ",
    "lerna:test:lib": "lerna run --stream test:lib ",
    "lerna:test:lib:watch": "lerna run --stream test:lib:watch",
    "lerna:typecheck": "lerna run --stream tsc:typecheck",
    "start": "yarn lerna:start:lib --scope @newrade/core-react-ui-*",
    "test": "yarn lerna:test:lib --scope @newrade/core-react-ui-*",
    "test:watch": "yarn lerna:test:lib:watch --scope @newrade/core-react-ui-*",
    "typecheck": "yarn lerna:typecheck --scope @newrade/core-react-ui-*"
  },
  "sideEffects": false,
  "type": "module",
  "dependencies": {}
}
