    "node": "^16.13"
  },
  "workspaces": {
    "nohoist": [],
    "packages": [
      "packages/core-types",
      "packages/core-common",
      "packages/core-jest-config",
      "packages/core-node-utils",
      "packages/core-webpack-config",
      "packages/core-figma-extractor",
